#include <stdio.h>

int main(){
    
    // intialize three numbers num1,num2,num3
    int num1, num2, num3;
    
    // read three numbers from the user
    printf("Max of three number is :  ");
    scanf("%d %d %d", &num1, &num2, &num3);
    
    // check which number is greatest
    if(num1 >= num2 && num1 >= num3){
    printf("Max of three number is :  %d\n", num1);
    }
    else if (num2 >= num1 && num2 >= num3){
    printf("Max of three number is : %d\n", num2);
    }
    else{
    printf("Max of three number is : %d\n", num3);
    }
    return 0;
}
