#include <stdio.h>
#include <ctype.h>
int main(){
    char ch;
    
    printf("Enter the character: ");
    scanf("%c", &ch);
    
    if(isupper(ch)){
        printf("The character is upper Case.\n");
    }
    else if(islower(ch)){
        printf("The character is Lower case.\n");
    }
    else if(isdigit(ch)){
        printf("The character is Digit.\n");
    }
    else{
        printf("The character not an alphabet or digit.\n");
    }
  return 0;
}
