#include <stdio.h>
int main(){
     
     //declare num1,num2
     int num1, num2; 
    
    //read one number num1 from the user
    printf("Enter the num1: ");
    scanf("%d", &num1);
    
    //read second number num2 from the user
    printf("Enter the num2: ");
    scanf("%d", &num2);
    
    //check which is greater using if condition
    if (num1 > num2){
        printf("Max of two numbers is: %d\n", num1);
    }
    else{
        printf("Max of two numbers is: %d\n", num2);
    }
    return 0;
 }
